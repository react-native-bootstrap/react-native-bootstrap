{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "eslint-config-prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/jsx-filename-extension": "off",
    "no-use-before-define": "off",
    "import/prefer-default-export": "warn",
    "react-hooks/exhaustive-deps": "warn"
  }
}